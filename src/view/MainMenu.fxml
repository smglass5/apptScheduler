<AnchorPane id="AnchorPane" maxHeight="400.0" maxWidth="400.0" prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #283545;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
   <children>
      <VBox alignment="CENTER" layoutX="52.0" layoutY="72.0" prefHeight="300.0" prefWidth="300.0" spacing="30.0">
         <children>
            <Button fx:id="handleCustomerButton" maxHeight="31.0" maxWidth="121.0" mnemonicParsing="false" onAction="#handleCustomer" text="Customer">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
            <Button fx:id="handleAppointmentButton" layoutX="178.0" layoutY="195.0" mnemonicParsing="false" onAction="#handleAppointment" text="Appointment">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
            <Button fx:id="handleReportButton" layoutX="178.0" layoutY="223.0" maxWidth="121.0" mnemonicParsing="false" onAction="#handleReport" text="Report">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
            <Button fx:id="handleExitButton" layoutX="156.0" layoutY="251.0" maxWidth="121.0" mnemonicParsing="false" onAction="#handleExit" text="Exit">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Button>
         </children>
      </VBox>
      <Label layoutX="137.0" layoutY="37.0" text="Main Menu" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </children>
   <effect>
      <InnerShadow />
   </effect>
</AnchorPane>
